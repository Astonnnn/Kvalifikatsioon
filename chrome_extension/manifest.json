{
  "manifest_version": 3,
  "name": "Website blocker",
  "description": "Blokeerib veebilehti vastavalt ajale",
  "version": "1.1",
  "permissions": ["activeTab","tabs", "webNavigation", "storage", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "https://localhost:5000/*",
    "http://127.0.0.1:5000/*",
    "<all_urls>"
  ]
}