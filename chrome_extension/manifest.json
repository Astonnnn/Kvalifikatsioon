{
  "manifest_version": 3,
  "name": "Website blocker",
  "description": "Blokeerib veebilehti vastavalt ajale",
  "version": "1.0",
  "permissions": ["tabs", "webNavigation", "storage", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "https://localhost:5000/"
  ],
  "action": {
    "default_popup": "block.html"
  }
}